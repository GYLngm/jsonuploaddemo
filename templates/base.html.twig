<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{{ title }}{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="/css/bootstrap.min.css" >
            <link rel="stylesheet" href="/css/main.css">
            <link rel="stylesheet" href="/css/font-awesome/css/font-awesome.min.css">
        {% endblock %}
    </head>
    <body>
    <nav class="navbar navbar-light bg-light justify-content-between">
        <div>Result: <p id="http_response"></p></div>
        <div style="text-align: right;">Click reset button to initialize table <i style="font-size:24px" class="fa">&#xf0a4;</i> <a href="{{ path('app_main_homepage') }}" class="btn btn-secondary btn-sm active" id="reset" role="button" aria-pressed="true">reset</a></div>
    </nav>
        {% block body %}
            <div class="container-fluid">
                <div class="section header-row shadow p-3 mb-5 bg-white body-content rounded">
                    <form id="form" enctype="multipart/form-data">
                        <div class="input-group mb-3">
                            <div class="custom-file">
                                <input type="file" class="custom-file-input" name="inputJsonFile01" id="inputJsonFile01">
                                <label class="custom-file-label" for="inputGroupFile01">Choose file</label>
                            </div>
                        </div>

                    </form>
                    <table class="table table-dark">
                        <thead>
                        <tr>
                            <th scope="col">Date</th>
                            <th scope="col">Time</th>
                            <th scope="col">Longitude</th>
                            <th scope="col">Latitude</th>
                            <th scope="col">IMEI</th>
                            <th scope="col">Version1</th>
                            <th scope="col">Temperature</th>
                            <th scope="col">Numero de bus</th>
                        </tr>
                        </thead>
                        <tbody id="tbody">

                        </tbody>
                    </table>
                </div>
            </div>
        {% endblock %}
        {% block javascripts %}
            <script type="text/javascript" src="/js/bootstrap.min.js"></script>
            <script type="text/javascript" src="/js/jquery.min.js"></script>
            <script type="text/javascript" src="/js/main.js"></script>
        {% endblock %}
    </body>
</html>
